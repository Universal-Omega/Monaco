{
	"name": "Monaco",
	"version": "2.8.0",
	"requires": {
		"MediaWiki": ">= 1.41.0"
	},
	"author": [
		"'''Universal Omega'''",
		"Inez Korczy≈Ñski",
		"Christian Williams",
		"Daniel Friesen",
		"James Haley",
		"Roger Meier"
	],
	"url": "https://www.mediawiki.org/wiki/Skin:Monaco",
	"descriptionmsg": "monaco-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "skin",
	"ValidSkinNames": {
		"monaco": {
			"displayname": "Monaco",
			"class": "SkinMonaco",
			"args": [ {
				"name": "monaco",
				"bodyOnly": true,
				"template": "MonacoTemplate",
				"styles": [
					"skins.monaco"
				],
				"scripts": [
					"skins.monaco.scripts"
				]
			} ]
		}
	},
	"MessagesDirs": {
		"Monaco": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"SkinMonaco": "includes/SkinMonaco.php",
		"MonacoHooks": "includes/MonacoHooks.php",
		"MonacoSidebar": "includes/MonacoSidebar.php",
		"MonacoTemplate": "includes/MonacoTemplate.php"
	},
	"ConfigRegistry": {
		"monaco": "GlobalVarConfig::newInstance"
	},
	"ResourceModules": {
		"skins.monaco": {
			"class": "MediaWiki\\ResourceLoader\\SkinModule",
			"features": {
				"elements": true,
				"content-links-external": true,
				"content-links": true,
				"content-media": true,
				"content-tables": true,
				"interface-message-box": true,
				"interface-category": true,
				"i18n-ordered-lists": true,
				"i18n-all-lists-margins": true,
				"i18n-headings": true
			},
			"targets": [
				"desktop",
				"mobile"
			],
			"position": "top",		
			"styles": {
				"style/css/print.css": {
					"media": "print"
				},
				"style/css/monobook_modified.css": {
					"media": "screen"
				},
				"style/css/reset_modified.css": {
					"media": "screen"
				},
				"style/css/root.css": {
					"media": "screen"
				},
				"style/css/header.css": {
					"media": "screen"
				},
				"style/css/article.css": {
					"media": "screen"
				},
				"style/css/widgets.css": {
					"media": "screen"
				},
				"style/css/footer.css": {
					"media": "screen"
				},
				"style/css/ny.css": {
					"media": "screen"
				},
				"style/css/buttons.css": {
					"media": "screen"
				},
				"style/css/sprite.css": {
					"media": "screen"
				},	
				"style/css/modal.css": {
					"media": "screen"
				},
				"style/css/star_rating.css": {
					"media": "screen"
				}
			}
		},
		"skins.monaco.scripts": {
			"scripts": "style/js/monaco.js"
		},
		"skins.monaco.widget": {
			"position" : "bottom",
			"scripts": "style/js/monacoWidget.js",
			"styles": {
				"style/css/monacoWidget.css": {
					"media": "screen"
				}
			}
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "Monaco"
	},
	"config": {
		"MonacoAllowUseTheme": {
			"value": true
		},
		"MonacoTheme": {
			"value": "sapphire"
		},
		"MonacoDynamicCreateOverride": {
			"value": false
		},
		"MonacoUseMoreButton": {
			"value": true
		},
		"MonacoUseSitenoticeIsland": {
			"value": false
		},
		"MonacoUseMasthead": {
			"value": true
		},
		"MonacoCompactSpecialPages": {
			"value": false
		},
		"MonacoBiggestCategoriesBlacklist": {
			"value": []
		},
		"MonacoEnablePaypal": {
			"value": false
		},
		"MonacoPaypalID": {
			"value": ""
		},
		"MonacoEnablePatreon": {
			"value": false
		},
		"MonacoPatreonURL": {
			"value": ""
		}
	},
	"Hooks": {
		"GetPreferences": "main",
		"MessageCacheReplace": [
			"MonacoSidebar::invalidateCache"
		],
		"OutputPageBodyAttributes": "main"
	},
	"HookHandlers": {
		"main": {
			"class": "MonacoHooks",
			"services": [
				"MainConfig",
				"UserOptionsLookup"
			]
		}
	},
	"manifest_version": 2
}
